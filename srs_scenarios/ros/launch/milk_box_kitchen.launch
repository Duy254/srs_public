<?xml version="1.0"?>
<launch>

	<!-- send cup to param server -->
	<param name="Milkbox" textfile="$(find cob_gazebo_worlds)/objects/milk_box.model" />

	<!-- spawn model in gazebo -->
	<node name="spawn_Milkbox" pkg="gazebo" type="spawn_model" args="-gazebo -param Milkbox -x -3.0 -y -0.2 -z 1.02 -R 0 -P 0 -Y 0 -model Milkbox" respawn="false" output="screen" />

</launch>

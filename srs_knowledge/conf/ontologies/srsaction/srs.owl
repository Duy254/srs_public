    <owl:Class rdf:about="http://www.srs-project.eu/ontologies/2011/11/srs.owl#MiddleLayerAction">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&srsaction;RobotAction"/>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.srs-project.eu/ontologies/2011/11/srs.owl#hasNextAction"/>
                                <owl:someValuesFrom rdf:resource="&srsaction;RobotAction"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.srs-project.eu/ontologies/2011/11/srs.owl#isNextActionOf"/>
                                <owl:someValuesFrom rdf:resource="&srsaction;RobotAction"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    



    <!-- http://www.srs-project.eu/ontologies/2011/11/srs.owl#BottomLayerAction -->

    <owl:Class rdf:about="http://www.srs-project.eu/ontologies/2011/11/srs.owl#BottomLayerAction">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.srs-project.eu/ontologies/2011/11/srs.owl#NoAction"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.srs-project.eu/ontologies/2011/11/srs.owl#hasNextAction"/>
                        <owl:allValuesFrom rdf:resource="http://www.srs-project.eu/ontologies/2011/11/srs.owl#NoAction"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


